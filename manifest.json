{
	"manifest_version": 2,
	"name": "Debug Panel",
	"description": "This extension provides read debug logs from web server with out integration in html",
	"homepage_url": "https://github.com/MihailDev/debug-panel",
	"version": "0.6",
	"permissions": [
        "<all_urls>",
        "tabs",
        "webRequest"
	],
	"icons": {
		"48": "/icons/128.png",
		"96": "/icons/128.png",
		"128": "/icons/128.png",
		"200": "/icons/200.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"devtools_page": "devtools.html"	
}